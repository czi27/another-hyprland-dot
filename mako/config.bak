# See man 5 mako

max-visible=3
sort=-time
output=HDMI-A-1
layer=top
anchor=top-right

font=monospace 10
#background-color=#282a36
#text-color=#d5d6d7

background-color=#000000
text-color=#85c1dc
border-color=#85c1dc
progress-color=over #414559

[urgency=high]
border-color=#ef9f76

width=200
height=90
margin=10
padding=5
border-size=0
#border-color=#657ABF
border-radius=2
progress-color=over #5588AAF0
icons=1
max-icon-size=64
icon-path=""
markup=1
actions=1
#format=<b>%s</b>\n%b Default when grouped: (%g) <b>%s</b>\n%b
default-timeout=3000
ignore-timeout=0
group-by=none
